{
  "name": "chicken-bro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "cross-env NODE_ENV=development node server/index.js",
    "server:prod": "cross-env NODE_ENV=production node server/index.js",
    "migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate:prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "migrate:undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "bootstrap": "^5.3.8",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.18.1",
    "pinia": "^3.0.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.5",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "uuid": "^13.0.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "cross-env": "^10.1.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
